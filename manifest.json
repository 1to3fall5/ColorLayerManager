{
    "id": "color.layer.manager",
    "name": "Color Layer Manager",
    "version": "1.0.0",
    "main": "index.html",
    "host": {
        "app": "PS",
        "minVersion": "25.0.0"
    },
    "manifestVersion": 4,
    "entrypoints": [
        {
            "type": "panel",
            "id": "colorLayerManager",
            "label": "颜色图层管理器",
            "minimumSize": {"width": 230, "height": 56},
            "preferredDockedSize": {"width": 230, "height": 56},
            "maximumSize": {"width": 2000, "height": 56},
            "icons": [
                {
                    "width": 23, 
                    "height": 23, 
                    "path": "icons/icon-dark.png", 
                    "scale": [1, 2],
                    "theme": ["darkest", "dark", "medium"]
                },
                {
                    "width": 23, 
                    "height": 23, 
                    "path": "icons/icon-light.png", 
                    "scale": [1, 2],
                    "theme": ["lightest", "light"]
                }
            ]
        }
    ],
    "icons": [
        {
            "width": 48,
            "height": 48,
            "path": "icons/plugin.png",
            "scale": [1, 2],
            "theme": ["darkest", "dark", "medium", "lightest", "light", "all"],
            "species": ["pluginList"]
        }
    ]
} 